{
  "version": 2,
  "builds": [
    {
      "src": "backend/vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/vercel_app.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "QDRANT_COLLECTION": "loan_policy_chunks",
    "ENVIRONMENT": "production"
  }
}
